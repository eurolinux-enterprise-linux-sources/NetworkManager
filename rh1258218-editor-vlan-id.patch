From b653fa7a24e2ab453dc4de57c32e2c90a79cfbeb Mon Sep 17 00:00:00 2001
From: Radek Vykydal <rvykydal@redhat.com>
Date: Thu, 7 Mar 2013 16:45:44 +0100
Subject: [PATCH] connection-editor: Fix allowed values for vlan id and MTU
MIME-Version: 1.0
Content-Type: text/plain; charset=UTF-8
Content-Transfer-Encoding: 8bit

https://bugzilla.redhat.com/show_bug.cgi?id=919113
Signed-off-by: Jiří Klimeš <jklimes@redhat.com>
---
 network-manager-applet-0.8.1/src/connection-editor/ce-page-vlan.ui | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/network-manager-applet-0.8.1/src/connection-editor/ce-page-vlan.ui b/network-manager-applet-0.8.1/src/connection-editor/ce-page-vlan.ui
index 081c8de..27b5265 100644
--- a/network-manager-applet-0.8.1/src/connection-editor/ce-page-vlan.ui
+++ b/network-manager-applet-0.8.1/src/connection-editor/ce-page-vlan.ui
@@ -2,7 +2,7 @@
 <interface>
   <!-- interface-requires gtk+ 3.0 -->
   <object class="GtkAdjustment" id="adjustment2">
-    <property name="upper">100</property>
+    <property name="upper">10000</property>
     <property name="step_increment">1</property>
     <property name="page_increment">10</property>
   </object>
@@ -106,7 +106,7 @@
             <property name="can_focus">True</property>
             <property name="invisible_char">●</property>
             <property name="invisible_char_set">True</property>
-            <property name="adjustment">vlan_id_adjustment</property>
+            <property name="adjustment">adjustment2</property>
             <property name="climb_rate">1</property>
           </object>
           <packing>
@@ -174,7 +174,7 @@
         <property name="invisible_char">●</property>
         <property name="width_chars">4</property>
         <property name="invisible_char_set">True</property>
-        <property name="adjustment">adjustment2</property>
+        <property name="adjustment">vlan_id_adjustment</property>
         <property name="numeric">True</property>
       </object>
       <packing>
-- 
2.1.0

